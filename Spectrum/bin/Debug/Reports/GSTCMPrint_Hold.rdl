    <DataSet Name="DSTAX">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BillNo">
            <Value>=Parameters!BillNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteCode">
            <Value>=Parameters!SiteCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECt isnull (TAX.TaxValue,0) AS TaxValue  ,SUM (isnull (A.GrossAmt,0))- SUM (isnull (A.LineDiscount,0)) AS TaxableAmt ,SUm (Isnull (A.TotalTaxAmount,0))/2 AS CGSTAmt 
,SUM(Isnull(A.TotalTaxAmount,0))/2 AS SGSTAmt,SUM (Isnull(A.TotalTaxAmount,0)) AS TotalTaxAmount
FROM CASHMEMODTL A LEFT JOIN (
SELECT Sum (isnull( MT.value,0)) TaxValue,BillLineNo from CashMemoTaxDtls CMt
InNER Join Msttax MT ON CMT.TaxLabel = MT.TaxCode and MT.STATUS =1 AND CMt.STATUS =1
WHERE BillNo = @BillNo AND SITECODE =@SiteCode
GROUP BY BillLineNo ) AS TAX On TAX.BilllineNo= A.BillLineNo
WHERE BillNo = @BillNo AND SITECODE =@SiteCode  
 GROUP BY TAX.TaxValue
 Having isnull (TAX.TaxValue,0) &gt;0</CommandText>
      </Query>
      <Fields>
        <Field Name="TaxValue">
          <DataField>TaxValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TaxableAmt">
          <DataField>TaxableAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CGSTAmt">
          <DataField>CGSTAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SGSTAmt">
          <DataField>SGSTAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalTaxAmount">
          <DataField>TotalTaxAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
