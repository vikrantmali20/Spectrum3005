    <DataSet Name="DSSubTotal">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BillNo">
            <Value>=Parameters!BillNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteCode">
            <Value>=Parameters!SiteCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
if exists (select * from sys.tables where name='SubTotalTable')          
begin          
DROP TABLE SubTotalTable    ;      
end 

 
 SELECt * Into SubTotalTable FROM (
 SELECt  'Sub Total' AS Details,  SUm (Isnull (GrossAmt ,0)) AS Amount
 from CASHMEMODTL WHERE BillNO=@BillNo AND SITECODE =@SiteCode AND Status=1
 UNION ALL

 SELECt  'Total Discount' AS Details, SUm (Isnull (TotalDiscount,0)) AS Amount
 from CASHMEMODTL WHERE BillNO= @BillNo AND SITECODE =@SiteCode AND Status=1
 HAving  SUm (Isnull (TotalDiscount,0))&gt;0
 UNION ALL

select Substring (TaxName,1,4) AS TAXName,Sum (Isnull(TaxValue,0)) AS TaxValue from CashMemoTaxDtls CMT
inner JOIN MSTTAX MT ON MT.TaxCode =CMT.TaxLabel WHERE BillNo = @BillNo 
AND SITECODE =@SiteCode
AND CMT.STATUS =1 AND MT.Status=1
GROUP By Substring (TaxName,1,4)
)  AS SUbTotalTbl

SELECt * from SubTotalTable</CommandText>
      </Query>
      <Fields>
        <Field Name="Details">
          <DataField>Details</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DSTotal">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 'Total' AS DETAILS,((
 isnull ((SELECt SUm (isnull ( Amount,0)) From SubTotalTable WHERE Details = 'Sub Total'),0) - 
isnull((SELECt SUm (isnull ( Amount,0)) From SubTotalTable WHERE Details = 'Total Discount'),0)) + isnull (
 (SELECt SUm (isnull ( Amount,0))From SubTotalTable WHERE Details = 'CGST' OR Details = 'SGST'),0)) AS Total
 
 UNION ALL
 SELECt * from (
 SELECT 'Round Off' AS DETAILS, (ROUND(((
 isnull ((SELECt SUm (isnull ( Amount,0)) From SubTotalTable WHERE Details = 'Sub Total'),0) - 
isnull((SELECt SUm (isnull ( Amount,0)) From SubTotalTable WHERE Details = 'Total Discount'),0)) + isnull (
 (SELECt SUm (isnull ( Amount,0))From SubTotalTable WHERE Details = 'CGST' OR Details = 'SGST'),0)),0)- ((
 isnull ((SELECt SUm (isnull ( Amount,0)) From SubTotalTable WHERE Details = 'Sub Total'),0) - 
isnull((SELECt SUm (isnull ( Amount,0)) From SubTotalTable WHERE Details = 'Total Discount'),0)) + isnull (
 (SELECt SUm (isnull ( Amount,0))From SubTotalTable WHERE Details = 'CGST' OR Details = 'SGST'),0))) AS Total
 ) AS Temp WHERE Temp.Total &lt;&gt;0
 </CommandText>
      </Query>
      <Fields>
        <Field Name="DETAILS">
          <DataField>DETAILS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DSTender">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BillNo">
            <Value>=Parameters!BillNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteCode">
            <Value>=Parameters!SiteCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TenderTypeCode,SUm (AmountTendered) AS AmountTendered from CashMemoReceipt WHERE BillNo = @BillNo   and STATUS =1
AND SITECODE =@SiteCode Group By TenderTypeCode having SUm (AmountTendered)&gt;0</CommandText>
      </Query>
      <Fields>
        <Field Name="TenderTypeCode">
          <DataField>TenderTypeCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AmountTendered">
