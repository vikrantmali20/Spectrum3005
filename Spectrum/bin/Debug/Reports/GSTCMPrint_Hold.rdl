        <CommandText>SELECt CMH.BillNo ,CMH.Billdate,CmH.CREATEDON ,AU.UserName AS CashierName ,
isnull (CLP.NameOnCard,'') AS Customername ,isnull (AddressLn1,'') + ' ' + ISNull (AddressLn2,'') AS CustomerAddress 
,  isnull (CityCode,'') AS City,isnull (CompanyName,'') AS CompanyName, isnull (PinCode ,'') AS PinCode ,
CASE WHEN   OrkutId is null or OrkutId= '' Then isnull (OrkutId ,'')  ELSE 'Buyer' +'''s GSTIN: '+ isnull (OrkutId ,'') END AS OrkutId
from CashMemoHdr  CMH
LEFT JOIN AuthUsers AU On AU.UserID  =CMH.CREATEDBY  and AU.STATUS =1 
LEFt JOIN CLPCustomers CLP ON CLP.CardNo =CMH.CLPNo  AND CLP.STATUS =1
LEFT JOIN CLPCustomerAddress CLPA ON  CLPA.CardNo =CMH.CLPNo AND CLPA.STATUS=1 and CLPA.DefaultAddress =1
WHERE BILLNO = @BillNo AND CMH.SITECODE =@SiteCode AND CMH.STATUS=1</CommandText>
